  </brick>
  <!--TetR promoter: induced by aTc (inhibits tetR), inhibited by tetR-->
  <brick id="PRTETR" size="54" type="promoter">
    <beta>75</beta>
    <!--instead, put reaction REPR2+ATC->REPR2*-->
    <formula>![0.01,2]REPR2</formula>
  </brick>
