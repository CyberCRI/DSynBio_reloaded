  <!--REPR1=lacI REPR2=tetR REPR3=cI REPR4=cI th-->

  <!--LacI promoter: induced by IPTG (inhibits lacI), inhibited by lacI/CAP proteins-->
  <brick id="PRLACI" size="200" type="promoter">
